{
  "hosting": {
    "public": "out",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "headers": [
      {
        "source": "**/*.@(css|js|png|jpg|jpeg|gif|svg|webp|woff|woff2|ttf|otf|eot|ttc|mp4|webm|ogg|mp3|wav|flac|aac)",
        "headers": [{ "key": "Cache-Control", "value": "max-age=31536000" }]
      }
    ]
  }
}
